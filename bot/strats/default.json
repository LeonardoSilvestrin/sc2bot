{
    "name": "default",
    "economy": {
        "scv_target": 20,
        "depot_trigger_supply_left": 2
    },
    "production": {
        "marine_cap": 24
    },
    "behaviors": {
        "macro": {
            "enabled": true,
            "auto_workers": true,
            "auto_scv": true,
            "auto_supply": true
        },
        "combat": {
            "enabled": false
        }
    },
    "drop": {
        "enabled": false
    },
    "build": [
        {
            "name": "depot_12",
            "when": {
                "supply_used_gte": 12
            },
            "do": {
                "build": "SUPPLYDEPOT"
            }
        },
        {
            "name": "barracks_after_depot",
            "requires": {
                "ready_gte": {
                    "SUPPLYDEPOT": 1
                }
            },
            "when": {
                "have_lte": {
                    "BARRACKS": 0
                },
                "minerals_gte": 150
            },
            "do": {
                "build": "BARRACKS"
            }
        },
        {
            "name": "refinery_after_rax",
            "requires": {
                "ready_gte": {
                    "BARRACKS": 1
                }
            },
            "when": {
                "have_lte": {
                    "REFINERY": 0
                }
            },
            "do": {
                "build": "REFINERY"
            }
        },
        {
            "name": "factory_after_rax",
            "requires": {
                "ready_gte": {
                    "BARRACKS": 1
                }
            },
            "when": {
                "have_lte": {
                    "FACTORY": 0
                },
                "minerals_gte": 150
            },
            "do": {
                "build": "FACTORY"
            }
        }
    ],
    "production_rules": [
        {
            "name": "marines",
            "requires": {
                "have_gte": {
                    "BARRACKS": 1
                }
            },
            "when": {
                "unit_lte": {
                    "MARINE": 24
                }
            },
            "do": {
                "train": "MARINE",
                "from": "BARRACKS",
                "cap": 24
            }
        }
    ]
}